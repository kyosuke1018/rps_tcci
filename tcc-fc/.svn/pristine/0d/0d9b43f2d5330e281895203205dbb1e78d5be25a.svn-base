package com.tcci.fc.util;

/**
 *
 * @author 
 */
public class TcException  extends Exception{

    public TcException() {
    }
    public TcException(String message) {
        super(message);
    }
    public String getMessage() {
        return super.getMessage();
    }
    public TcException(Exception e) {
        super(e);
    }

    public TcException(Throwable e) {
        super(e);
    }

    public TcException(String message, Throwable e) {
        super(message, e);
    }

}