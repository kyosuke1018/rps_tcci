package com.tcci.fc.controller.bpm;

import java.math.BigDecimal;
import java.util.Date;

/**
 *
 * @author Greg.Chou
 */
public class HumanTask {
    private long id;
    private String applicationName;
    private String applicationId;
    private String subject;
    private int priority;
    private String approver;
    private String requester;
    private Date processStart;
    private String formId;
    private String formURL;
    private BigDecimal processHour;
    private String processTimeDisplay;
    private WorkflowStatus status;

    private String taskName;

    public HumanTask() {
    }

    public HumanTask(long id, String name, String subject, int priority, String approver, String requester, Date processStart) {
        this.id = id;
        this.applicationName = name;
        this.subject = subject;
        this.priority = priority;
        this.approver = approver;
        this.requester = requester;
        this.processStart = processStart;
    }

    public String getApprover() {
        return approver;
    }

    public void setApprover(String approver) {
        this.approver = approver;
    }

    public long getId() {
        return id;
    }

    public void setId(long id) {
        this.id = id;
    }

    public String getApplicationName() {
        return applicationName;
    }

    public void setApplicationName(String name) {
        this.applicationName = name;
    }

    public int getPriority() {
        return priority;
    }

    public void setPriority(int priority) {
        this.priority = priority;
    }

    public Date getProcessStart() {
        return processStart;
    }

    public void setProcessStart(Date processStart) {
        this.processStart = processStart;
    }

    public String getRequester() {
        return requester;
    }

    public void setRequester(String requester) {
        this.requester = requester;
    }

    public String getSubject() {
        return subject;
    }

    public void setSubject(String subject) {
        this.subject = subject;
    }

    public BigDecimal getProcessHour() {
        return processHour;
    }

    public void setProcessHour(BigDecimal processHour) {
        this.processHour = processHour;
    }

    public String getFormURL() {
        return formURL;
    }

    public void setFormURL(String formURL) {
        this.formURL = formURL;
    }

    public String getApplicationId() {
        return applicationId;
    }

    public void setApplicationId(String applicationId) {
        this.applicationId = applicationId;
    }

    public String getProcessTimeDisplay() {
        return processTimeDisplay;
    }

    public void setProcessTimeDisplay(String processTimeDisplay) {
        this.processTimeDisplay = processTimeDisplay;
    }

    public String getFormId() {
        return formId;
    }

    public void setFormId(String formId) {
        this.formId = formId;
    }

    public WorkflowStatus getStatus() {
        return status;
    }

    public void setStatus(WorkflowStatus status) {
        this.status = status;
    }

    public String getTaskName() {
        return taskName;
    }

    public void setTaskName(String taskName) {
        this.taskName = taskName;
    }
}
