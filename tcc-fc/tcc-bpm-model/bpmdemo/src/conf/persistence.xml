<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
    <persistence-unit name="Model" transaction-type="JTA">
        <jta-data-source>jdbc/bpmdemo</jta-data-source>
        <class>com.tcci.fc.entity.org.TcUser</class>
        <class>com.tcci.fc.entity.org.TcGroup</class>
        <class>com.tcci.fc.entity.org.TcUsergroup</class>
        <class>com.tcci.fc.entity.essential.TcDomain</class>
        <class>com.tcci.fc.entity.content.TcApplicationdata</class>
        <class>com.tcci.fc.entity.content.TcFvitem</class>
        <class>com.tcci.fc.entity.content.TcFvvault</class>
        <!-- tcc-bpm-model {{ -->
        <class>com.tcci.fc.entity.bpm.TcActivity</class>
        <class>com.tcci.fc.entity.bpm.TcActivityroute</class>
        <class>com.tcci.fc.entity.bpm.TcActivityroutetemplate</class>
        <class>com.tcci.fc.entity.bpm.TcActivitytemplate</class>
        <class>com.tcci.fc.entity.bpm.TcProcess</class>
        <class>com.tcci.fc.entity.bpm.TcProcesstemplate</class>
        <class>com.tcci.fc.entity.bpm.TcSignature</class>
        <class>com.tcci.fc.entity.bpm.TcVariable</class>
        <class>com.tcci.fc.entity.bpm.TcWorkitem</class>
        <class>com.tcci.fc.entity.role.TcRole</class>
        <class>com.tcci.fc.entity.role.TcRoleholderrolemap</class>
        <class>com.tcci.fc.entity.role.TcRoleprincipallink</class>
        <class>com.tcci.fc.entity.team.TcTeam</class>
        <!-- }} -->
        <exclude-unlisted-classes>true</exclude-unlisted-classes>
        <properties>
            <!-- JPA logging -->
            <property name="eclipselink.logging.level" value="WARNING"/>
            <property name="eclipselink.logging.parameters" value="false"/>
        </properties>
    </persistence-unit>
</persistence>
