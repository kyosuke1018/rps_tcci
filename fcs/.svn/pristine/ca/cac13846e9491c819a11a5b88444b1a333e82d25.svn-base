package com.tcci.fcs.util;

/**
 *
 * @author Jimmy.Lee
 */
public class ExcelAccountItem {
    private String col;  // excel欄位
    private String code; // 會科代碼
    private String desc; // 會科名稱
    private String edesc; // 會科名稱(英文)

    public ExcelAccountItem(String col, String code, String desc) {
        this.col = col;
        this.code = code;
        this.desc = desc;
    }
    
    public ExcelAccountItem(String col, String code, String desc, String edesc) {
        this.col = col;
        this.code = code;
        this.desc = desc;
        this.edesc = edesc;
    }
    
    // getter, setter
    public String getCol() {
        return col;
    }

    public void setCol(String col) {
        this.col = col;
    }

    public String getCode() {
        return code;
    }

    public void setCode(String code) {
        this.code = code;
    }

    public String getDesc() {
        return desc;
    }

    public void setDesc(String desc) {
        this.desc = desc;
    }

    public String getEdesc() {
        return edesc;
    }

    public void setEdesc(String edesc) {
        this.edesc = edesc;
    }

}
