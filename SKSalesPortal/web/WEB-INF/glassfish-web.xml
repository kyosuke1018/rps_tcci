<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE glassfish-web-app PUBLIC "-//GlassFish.org//DTD GlassFish Application Server 3.1 Servlet 3.0//EN" "http://glassfish.org/dtds/glassfish-web-app_3_0-1.dtd">
<glassfish-web-app error-url="" httpservlet-security-provider="SAM_SKSP">
    <context-root>/SKSalesPortal</context-root>
    <class-loader delegate="true"/>
    <jsp-config>
        <property name="keepgenerated" value="true">
            <description>Keep a copy of the generated servlet class' java code.</description>
        </property>   
    </jsp-config>
    <locale-charset-info default-locale="">
        <locale-charset-map locale="" charset=""/>
        <parameter-encoding default-charset="UTF-8"/>
    </locale-charset-info>         
    <security-role-mapping>
        <role-name>valid-users</role-name>
        <group-name>Authenticated</group-name>
    </security-role-mapping>
    <security-role-mapping>
        <role-name>Sales</role-name>
        <group-name>Sales</group-name>
    </security-role-mapping>
    <security-role-mapping>
        <role-name>Finance</role-name>
        <group-name>Finance</group-name>
    </security-role-mapping>
    <security-role-mapping>
        <role-name>Assistant</role-name>
        <group-name>Assistant</group-name>
    </security-role-mapping>
    <security-role-mapping>
        <role-name>Administrators</role-name>
        <group-name>ADMINISTRATORS</group-name>
    </security-role-mapping>
    <security-role-mapping>
        <role-name>Managers</role-name>
        <group-name>Managers</group-name>
    </security-role-mapping>
    <security-role-mapping>
        <role-name>ComplainEditor</role-name>
        <group-name>ComplainEditor</group-name>
    </security-role-mapping>
    <security-role-mapping>
        <role-name>Planner</role-name>
        <group-name>Planner</group-name>
    </security-role-mapping>
</glassfish-web-app>
