  color: #909090;
}

.tar {
    text-align: right;
}

.tac {
    text-align: center;
}

.vam {
    vertical-align: middle;  
}

.pt100 {
    width: 100%;
}

.selPayMethod{
    min-width: 100px; 
}

table.tbRfqItems {
    width: 96%; 
}

table.tbRfqItems th {
    background-color: rgba(160, 200, 160, 0.85);
    min-width: 60px; 
    font-weight: bold;
    font-size: 14px;
    text-align: center;  
}

table.tbRfqItems td {
    padding: 3px;
    font-size: 14px;
}

table.tbRfqItems tr.trOdd {
    background-color: rgba(204, 240, 204, 0.75);
}

table.tbSrvItems th {
    background-color: rgba(160, 160, 220, 0.85);
    min-width: 60px; 
    font-weight: bold;
    font-size: 14px;
    text-align: center;  
}

.divCalResult {
    border:  #eeaa99 thin solid;
    padding: 5px; 
    margin-top: 10px;
    margin-bottom: 20px;
}

.divBtns {
    width: 100%; 
    text-align: center;
}

.cover {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0px;
    z-index: -1;
    background: url(../img/cover_login.jpg) center 20% no-repeat;
    background-size: cover;
}

.form {
    width:90%;
    min-width: 800px;
    margin: 50px auto 30px;
    padding: 30px;
    box-sizing: border-box;
    background: rgba(255, 255, 255, 0.75);
    box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
}

.form .caption {
    margin: 0 0 10px;
    font-weight: bold;
    font-size: 28px;
    color: #808080;
}

.form .caption .note {
    font-weight: normal;
    font-size: 16px;
}

.form ul {
    padding: 0;
    list-style: none;    
}

.form ul li {
    width: 100%;
    margin: 0;
    padding: 0 3%;
    box-sizing: border-box;
    float: left;
}

.form ul .w33 {
    width: 33%;
    padding: 0 3%;
    box-sizing: border-box;
}

.form ul .w50 {
    width: 50%;
    padding: 0 3%;
    box-sizing: border-box;
}

.form ul li .nonTxtTitle {
    margin: 15px 0 0;
    font-size: 12px;
    color: rgba(204, 0, 0, 0.75);    
}

.form ul li select {
    width: 180px;
    margin: 5px 0 15px 10px;
    padding: 2px 10px;
}

.form ul li .multiSelectNote {
    padding: 40px 10px;
    display: inline-block;
    vertical-align: top;
}

.form ul li .chkboxGroup > div {
    padding: 10px 0 0 20px;
    float: left;
}

.form ul li .chkboxGroup > div input[type="input"] {
    width: 185px;
    display: inline-block;
    border: none;
    border-bottom: dotted 1px #808080;
    font-size: 16px;
    background: none;
    outline: none;
}

.form .btn {
    width: 200px;
    margin: 20px auto 0;
    padding: 7px 10px;
    box-sizing: border-box;
    border-radius: 2px;
    text-align: center;
    color: #fff;
    background: #b00000;
    cursor: pointer;
}

