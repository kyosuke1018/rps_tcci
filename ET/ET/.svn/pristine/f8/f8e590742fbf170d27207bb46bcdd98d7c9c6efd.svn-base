.cover {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0px;
    z-index: -1;
    background: url(../img/cover_login.jpg) center 20% no-repeat;
    background-size: cover;
}

.contentTopSpacing {
    height: 100px;
}



#member {
    opacity: 0;
}

#member .btn {
    padding: 5px 10px;
    display: inline;
    border: solid 1px #c0c0c0;
    border-radius: 1px;
    vertical-align: middle;
    font-size: 14px;
    color: #b00;
    background: #fff;
    cursor: pointer;
}

#member .btn:hover {    
    text-shadow: 0 0 5px rgba(187, 0, 0, 0.25);
}

#member .btn:active {
    box-shadow: 1px 1px 3px rgba(0, 0, 0, 0.5) inset;
}



.memberInfo {
    margin: 0 0 80px;
    padding: 10px 60px;
    box-sizing: border-box;    
    background: rgba(255, 255, 255, 0.5);
    box-shadow: 0 0 10px rgba(0, 0, 0, 0.2);
}

.memberInfo .baseInfo {
    padding: 0;
    list-style: none;
}

.memberInfo .baseInfo li {
    width: 100%;
    margin: 0;
    padding: 10px 0;
    box-sizing: border-box;
    float: left;    
}

.memberInfo .baseInfo .w33 {
    width: 33%;
    display: inline-block;
}

.memberInfo .baseInfo li .title {
    display: inline-block;
    font-weight: bold;
    font-size: 14px;
    color: #b00;
}

.memberInfo .baseInfo li .title:before {
    content: "â€¢ "
}

.memberInfo .baseInfo li .data {
    padding: 0 0 0 12px;
    box-sizing: border-box;
}



.bindSupplier {
    margin: 0 0 80px;
}

.bindSupplier .tableTitle {
    border-top: solid 1px rgba(0, 0, 0, 0.1);
    background: rgba(200, 200, 200, 0.7);
}

.bindSupplier .tableTitle div {
    padding: 10px 20px;
    box-sizing: border-box;
    display: inline-block;
    font-weight: bold;
}

.bindSupplier .tableTitle .block1 div:nth-child(1) {
    width: 20%;
}

.bindSupplier .tableTitle .block2 div:nth-child(1) {
    width: 15%;
}

.bindSupplier .tableTitle .block2 div:nth-child(2) {
    width: 40%;
}

.bindSupplier .tableTitle .block3 div:nth-child(1) {
    width: 24%;
}

.bindSupplier .row {
    border-bottom: dotted 1px #c0c0c0;
}

.bindSupplier .row:nth-child(even) {
    background: rgba(255, 255, 255, 0.5);
}

.bindSupplier .row:nth-child(odd) {
    background: rgba(255, 255, 255, 0.7);
}

.bindSupplier .row div {
    padding: 15px 20px;
    box-sizing: border-box;
    display: inline-block;    
}

.bindSupplier .row .block1 div:nth-child(1) {
    width: 20%;
}

.bindSupplier .row .block2 div:nth-child(1) {
    width: 15%;
}

.bindSupplier .row .block2 div:nth-child(2) {
    width: 40%;
    vertical-align: bottom;
    overflow: hidden;
    white-space : nowrap;
    text-overflow : ellipsis;
}

.bindSupplier .row .block3 div:nth-child(1) {
    width: 24%;
    vertical-align: bottom;
    overflow: hidden;
    white-space : nowrap;
    text-overflow : ellipsis;
}



.bindNewSupplier {
    margin: 0 0 30px;
}

.bindNewSupplier .tableTitle {
    border-top: solid 1px rgba(0, 0, 0, 0.1);
    background: rgba(200, 200, 200, 0.7);
}

.bindNewSupplier .tableTitle div {
    padding: 10px 20px;
    box-sizing: border-box;
    display: inline-block;
    font-weight: bold;
}

.bindNewSupplier .tableTitle .block1 div:nth-child(1) {
    width: 15%;
}

.bindNewSupplier .tableTitle .block2 div:nth-child(1) {
    width: 15%;
}

.bindNewSupplier .tableTitle .block2 div:nth-child(2) {
    width: 43%;
}

.bindNewSupplier .tableTitle .block3 div:nth-child(1) {
    width: 15%;
}

.bindNewSupplier .tableTitle .block3 div:nth-child(2) {
    width: 10%;
}

.bindNewSupplier .row {
    border-bottom: dotted 1px #c0c0c0;
}

.bindNewSupplier .row:nth-child(even) {
    background: rgba(255, 255, 255, 0.5);
}

.bindNewSupplier .row:nth-child(odd) {
    background: rgba(255, 255, 255, 0.7);
}

.bindNewSupplier .row div {
    padding: 15px 20px;
    box-sizing: border-box;
    display: inline-block;    
}

.bindNewSupplier .row .block1 div:nth-child(1) {
    width: 15%;
}

.bindNewSupplier .row .block2 div:nth-child(1) {
    width: 15%;
}

.bindNewSupplier .row .block2 div:nth-child(2) {
    width: 43%;
    vertical-align: bottom;
    overflow: hidden;
    white-space : nowrap;
    text-overflow : ellipsis;
}

.bindNewSupplier .row .block3 div:nth-child(1) {
    width: 15%;
}

.bindNewSupplier .row .block3 div:nth-child(2) {
    width: 10%;
}



