<ui:composition 
    xmlns="http://www.w3.org/1999/xhtml"
    xmlns:f="http://java.sun.com/jsf/core"
    xmlns:h="http://java.sun.com/jsf/html"
    xmlns:ui="http://java.sun.com/jsf/facelets"
    xmlns:p="http://primefaces.org/ui">

    <div class="topbar clearfix">
        <a id="omega-menu-button" href="#">
            <span class="fa fa-bars"></span>
        </a>
        
        <!--<h:outputLink value="#{facesContext.externalContext.request.contextPath}#{tcSessionController.homePage}">-->
        <h:outputLink value="#{facesContext.externalContext.request.contextPath}">
        <span title="#{app.serverIpAddress}" class="topbar-title" style="color:white;">
            <h:outputText value="#{msg['header.app_name']}"/>
        </span>
        </h:outputLink>
        
        <a id="options-menu-button" href="#">
            <span class="fa fa-ellipsis-h"></span>
        </a>
        
        <div id="topbar-icons" class="animated">
            <span class="topbar-search">
                <h:outputText value="您好: #{tcSessionController.loginTcUser.displayIdentifier}"/>
            </span>
            <!--<a href="/et-admin/faces/logout.xhtml" >-->
            <a href="/et-admin/faces#{tcSessionController.logoutPage}.xhtml" >
                <span class="topbar-item-text">#{msg['header.logout']}</span>
                <span class="topbar-icon fa fa-sign-out"></span>
            </a>
        </div>
        
    </div>
    
</ui:composition>