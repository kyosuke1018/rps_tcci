<?xml version='1.0' encoding='UTF-8' ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<ui:composition xmlns="http://www.w3.org/1999/xhtml"
                xmlns:ui="http://java.sun.com/jsf/facelets"
                xmlns:f="http://java.sun.com/jsf/core"
                xmlns:h="http://java.sun.com/jsf/html"
                xmlns:p="http://primefaces.org/ui"
                template="/templates/generalPageTemplate.xhtml">
    <ui:define name="title">送達地點匯入</ui:define>
    <ui:define name="content">
        <p:panel header="送達地點匯入">
            <ui:include src="inc_submenu.xhtml"/>
            <h:form id="form" prependId="false">
                <ui:include src="/include/inc_importExcel.xhtml">
                    <ui:param name="importController" value="#{deliveryImport}"/>
                    <ui:param name="excelTemplateURL" value="/templates/deliveryImport.xlsx"/>
                </ui:include>
            </h:form>
        </p:panel>
    </ui:define>
    <ui:define name="voColumns">
        <p:column headerText="代碼"
                  sortBy="#{row.code}">
            <h:outputText value="#{row.code}"/>
        </p:column>
        <p:column headerText="名稱"
                  sortBy="#{row.name}">
            <h:outputText value="#{row.name}"/>
        </p:column>
        <p:column headerText="省"
                  sortBy="#{row.province}">
            <h:outputText value="#{row.province}"/>
        </p:column>
        <p:column headerText="市"
                  sortBy="#{row.city}">
            <h:outputText value="#{row.city}"/>
        </p:column>
        <p:column headerText="區"
                  sortBy="#{row.district}">
            <h:outputText value="#{row.district}"/>
        </p:column>
        <p:column headerText="鎮"
                  sortBy="#{row.town}">
            <h:outputText value="#{row.town}"/>
        </p:column>
        <p:column headerText="銷售地區"
                  sortBy="#{row.salesarea}">
            <h:outputText value="#{row.salesarea}"/>
        </p:column>
        <p:column headerText="銷售組織銷售區域"
                  sortBy="#{row.vksas}">
            <h:outputText value="#{row.vksas}"/>
        </p:column>
        <p:column headerText="啟用"
                  sortBy="#{row.active}">
            <h:outputText value="#{row.active}"/>
        </p:column>
    </ui:define>
</ui:composition>