package com.tcci.tccstore.model.contract;

/**
 *
 * @author Jimmy.Lee
 */
public class Contract {
    
    private Long id;
    private String code;
    private String name;

    public Contract() {
    }
    
    // getter, setter
    public Long getId() {
        return id;
    }

    public void setId(Long id) {
        this.id = id;
    }

    public String getCode() {
        return code;
    }

    public void setCode(String code) {
        this.code = code;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

}
