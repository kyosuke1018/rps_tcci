# disable static users
#cas.authn.accept.users=casadmin::Abcd1234
cas.authn.accept.users=

# service registry
cas.serviceRegistry.initFromJson=true
cas.serviceRegistry.json.location: file:/opt/FileUpload/cas/services

# authentication control
#cas.authn.policy.any.tryAll=false
#cas.authn.policy.all.enabled=true

#ldap authentication
cas.authn.ldap[0].name=LDAP_AUTH
cas.authn.ldap[0].order=1
cas.authn.ldap[0].type=AD
cas.authn.ldap[0].ldapUrl=${com.taiwancement.sso.ldapUrl}
cas.authn.ldap[0].useSsl=false
cas.authn.ldap[0].useStartTls=false
cas.authn.ldap[0].connectTimeout=5000
cas.authn.ldap[0].connectionStrategy=DEFAULT
cas.authn.ldap[0].baseDn=DC=taiwancement,DC=com
cas.authn.ldap[0].subtreeSearch=true
cas.authn.ldap[0].bindDn=${com.taiwancement.sso.bindDn}
cas.authn.ldap[0].bindCredential=${com.taiwancement.sso.bindCredential}
cas.authn.ldap[0].dnFormat=%s@taiwancement.com
cas.authn.ldap[0].failFast=false
cas.authn.ldap[0].idleTime=5000
cas.authn.ldap[0].prunePeriod=5000
cas.authn.ldap[0].blockWaitTime=5000
cas.authn.ldap[0].searchFilter=(&(objectCategory=person)(objectClass=user))
#cas.authn.ldap[0].principalAttributeId=
#cas.authn.ldap[0].principalAttributePassword=
#cas.authn.ldap[0].principalAttributeList=userPrincipalName,cn,mail
#cas.authn.ldap[0].allowMultiplePrincipalAttributeValues=true
#cas.authn.ldap[0].allowMissingPrincipalAttributeValue=true

#db authentication
cas.authn.jdbc.query[0].name=DB_AUTH
cas.authn.jdbc.query[0].order=2
cas.authn.jdbc.query[0].sql=SELECT * FROM V_EMPLOYEE_SSO WHERE SSO_ACCOUNT=?
cas.authn.jdbc.query[0].fieldPassword=PASSWORD
cas.authn.jdbc.query[0].user=${com.taiwancement.sso.user}
cas.authn.jdbc.query[0].password=${com.taiwancement.sso.password}
cas.authn.jdbc.query[0].driverClass=oracle.jdbc.OracleDriver
cas.authn.jdbc.query[0].url=${com.taiwancement.sso.url}
cas.authn.jdbc.query[0].dialect=org.hibernate.dialect.Oracle12cDialect
cas.authn.jdbc.query[0].passwordEncoder.type=DEFAULT
cas.authn.jdbc.query[0].passwordEncoder.encodingAlgorithm=SHA256
cas.authn.jdbc.query[0].passwordEncoder.characterEncoding=UTF-8
cas.authn.jdbc.query[0].principalAttributeList=company_code

#ticket registry (hazelcast)
cas.ticket.registry.hazelcast.cluster.members=${com.taiwancement.sso.hazelcasthosts}
cas.ticket.registry.hazelcast.cluster.instanceName=localhost
cas.ticket.registry.hazelcast.cluster.port=5701
cas.ticket.registry.hazelcast.cluster.tcpipEnabled=true
cas.ticket.registry.hazelcast.cluster.partitionMemberGroupType=PER_MEMBER

#ticket expiration policy
#tgt
cas.ticket.tgt.maxTimeToLiveInSeconds=0
cas.ticket.tgt.hardTimeout.timeToKillInSeconds=32400
#st
cas.ticket.st.numberOfUses=1
cas.ticket.st.timeToKillInSeconds=10
#tgc cookie
cas.tgc.path=
cas.tgc.maxAge=86400
cas.tgc.domain=
cas.tgc.name=TGC
cas.tgc.secure=false
cas.tgc.httpOnly=true
cas.tgc.rememberMeMaxAge=1209600

#ouath
#cas.authn.pac4j.cas[0].loginUrl=${com.taiwancement.sso.empid.loginUrl}
#cas.authn.pac4j.cas[0].clientName=EMP ID
##CAS10, CAS20, CAS20_PROXY, CAS30, CAS30_PROXY, SAML
#cas.authn.pac4j.cas[0].protocol=CAS30
#cas.authn.pac4j.cas[0].usePathBasedCallbackUrl=true

cas.authn.pac4j.cas[1].loginUrl=${com.taiwancement.sso.ccil.loginUrl}
cas.authn.pac4j.cas[1].clientName=CCIL AD
#CAS10, CAS20, CAS20_PROXY, CAS30, CAS30_PROXY, SAML
cas.authn.pac4j.cas[1].protocol=CAS30
cas.authn.pac4j.cas[1].usePathBasedCallbackUrl=true

#ui
cas.view.templatePrefixes[0]=file:/opt/FileUpload/cas/templates/2019_dumpling

cas.messageBundle.commonNames=classpath:/common_messages.properties,file:/opt/FileUpload/cas/config/common_messages.properties