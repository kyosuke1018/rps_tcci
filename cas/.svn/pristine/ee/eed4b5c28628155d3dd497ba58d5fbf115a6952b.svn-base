<?xml version='1.0' encoding='UTF-8' ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:h="http://xmlns.jcp.org/jsf/html"
      xmlns:f="http://xmlns.jcp.org/jsf/core">
    <f:metadata>
        <f:viewParam name="customerId" value="#{home.id1}" />
        <f:event listener="#{home.load()}" type="preRenderView"/>
    </f:metadata>

    <h:head>
        <title>Demo JavaEE 8 Security 1.0 API</title>
    </h:head>
    <h:body>
        login user = #{home.user}<br/>
        user in [ROLE_USER] group = #{home.hasUserRole}<br/>
        user in [ROLE_ADMIN] group = #{home.hasAdminRole}<br/>
        
        ID : <h:outputText value="#{home.id1}" /><br/>
        <br/><br/>
        <a href="#{request.contextPath}/logout" >Logout</a>
    </h:body>
</html>

